        --REAL = TESTVAL1			; Test passes if Object1 property P1 = TESTVAL1
       REAL = INITVAL1                          ; if Property Write not supported
      CloseTag 3
    )

   -- Step #6: Verify the current value of property P2

    SEND (
      NETWORK = "Untitled"
      DA = IUT_IP
