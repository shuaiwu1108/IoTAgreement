        OpenTag 2
            REAL = TESTVAL1
        CloseTag 2
        Property = 0, COV-Increment
        OpenTag 2
            REAL = TESTVAL2
        CloseTag 2
       CloseTag 1                                   
    )

    EXPECT (
      NETWORK = "Untitled"
      SA = IUT_IP
      DER = FALSE
      BVLCI = ORIGINAL-UNICAST-NPDU
      PDU = SimpleAck
      Service = WritePropertyMultiple
    )
   OR(
      NETWORK = "Untitled"
      SA = IUT_IP
      DER = FALSE
      BVLCI = ORIGINAL-UNICAST-NPDU
      PDU = error
      Error-Choice = WritePropertyMultiple
      OpenTag 0
       Enum = 2               ;Property
       Enum = 40              ;Write-Access-Denied
      CloseTag 0
      OpenTag 1
       Object = 0,OBJECT1  
       Property = 1,Relinquish-Default
      CloseTag 1
    )

  -- Step #5: Verify the current value of property P1
 
    SEND (
