      PDU = Confirmed-Request
      Service = GETENROLLMENTSUMMARY
      Object EVENT_ID
      ENUM EVENT_TYPE_OUT_OF_RANGE
      ENUM EVENT_STATE_NORMAL
      INTEGER NORMAL_PRIORITY
      )

  -- 5. TRANSMIT GetEnrollmentSummary-Request
  --        'Acknowledgment Filter' =  ALL,
  --        'MinPriority' = XLOW
  --        'MaxPriority' = XHIGH

    SEND (
      NETWORK = "Untitled"
